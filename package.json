{
  "name": "@cdssnc/elenchos",
  "version": "0.0.1",
  "description": "CDS PR deploys",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && node index",
    "lint": "eslint src/*.js",
    "test": "tsc && jest --forceExit",
    "start": "tsc && NODE_ENV=production node index",
    "build": "tsc",
    "watch": "tsc && tsc-watch",
    "minikube": "kustomize build manifests/overlays/minikube | kubectl apply -f -",
    "delete-minikube": "kustomize build manifests/overlays/minikube | kubectl delete -f -",
    "gke": "kustomize build manifests/overlays/gke | kubectl apply -f -",
    "delete-gke": "kustomize build manifests/overlays/gke | kubectl delete -f -"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/jest": "^24.0.12",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^2.1.2",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@cdssnc/logdriver": "^0.0.2",
    "@octokit/app": "^2.2.4",
    "@octokit/rest": "^16.25.3",
    "@types/express": "^4.16.1",
    "@types/js-yaml": "^3.12.1",
    "@types/mongoose": "^5.5.0",
    "@types/node-fetch": "^2.3.3",
    "body-parser": "^1.19.0",
    "dotenv-safe": "^6.1.0",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "js-yaml": "^3.13.1",
    "mongoose": "^5.5.6",
    "node-fetch": "^2.5.0"
  }
}
